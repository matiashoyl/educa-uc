<%- model_class = Evaluation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @evaluation.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:school_id) %>:</strong></dt>
  <dd><%= @evaluation.school_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:personal_members_present) %>:</strong></dt>
  <dd><%= @evaluation.personal_members_present %></dd>
  <dt><strong><%= model_class.human_attribute_name(:number_kids) %>:</strong></dt>
  <dd><%= @evaluation.number_kids %></dd>
  <dt><strong><%= model_class.human_attribute_name(:max_number_kids) %>:</strong></dt>
  <dd><%= @evaluation.max_number_kids %></dd>
  <dt><strong><%= model_class.human_attribute_name(:evaluation_date) %>:</strong></dt>
  <dd><%= @evaluation.evaluation_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:number_kids_disability) %>:</strong></dt>
  <dd><%= @evaluation.number_kids_disability %></dd>
  <dt><strong><%= model_class.human_attribute_name(:physical_disability) %>:</strong></dt>
  <dd><%= @evaluation.physical_disability %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cognitive_disability) %>:</strong></dt>
  <dd><%= @evaluation.cognitive_disability %></dd>
  <dt><strong><%= model_class.human_attribute_name(:social_disability) %>:</strong></dt>
  <dd><%= @evaluation.social_disability %></dd>
  <dt><strong><%= model_class.human_attribute_name(:other_disability) %>:</strong></dt>
  <dd><%= @evaluation.other_disability %></dd>
  <dt><strong><%= model_class.human_attribute_name(:min_birth_date) %>:</strong></dt>
  <dd><%= @evaluation.min_birth_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:max_birth_date) %>:</strong></dt>
  <dd><%= @evaluation.max_birth_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:start_time) %>:</strong></dt>
  <dd><%= @evaluation.start_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:finish_time) %>:</strong></dt>
  <dd><%= @evaluation.finish_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:scale_id) %>:</strong></dt>
  <dd><%= @evaluation.scale_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              evaluations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_evaluation_path(@evaluation), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              evaluation_path(@evaluation),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
